digimon matematicasmon{
	digital numeromon1;
	digital numeromon2;
	
	numeromon1 = 3;
	numeromon2 = 3.5;
	digitalk "------";
    digitalk "pruebas con numeros";
	
    digitalk 1-3;
    digitalk 1*2;
    digitalk 2/2;
    digitalk 5%4;
    digitalk ((1+1)*2)/4;
    
    digitalk "------";
    digitalk "pruebas con variables: ";
    digitalk "variable 1: ";
    digitalk numeromon1;
    digitalk "variable 2: ";
    digitalk numeromon2;
    digitalk "------";
    
    digitalk "Variable 1 + Variable 2: ";
    digitalk numeromon1;
    digitalk "+";
    digitalk numeromon2;
    digitalk "=";
    
    digitalk numeromon1 + numeromon2;
    
}
